#python
#quick sortt

def quickSort(arr,b,e):
    if e-b <= 1:
        return None
    p = e-1
    f = b
    for l in range(b,e-1):
        if arr[l]<arr[p]:
            arr[l],arr[f] = arr[f],arr[l]
            f+=1
    arr[p] ,arr[f] = arr[f],arr[p]

    quickSort(arr,b,f)
    quickSort(arr,f,e)
    return None

arr = [5,4,3,6,7,8,2,9,10]
quickSort(arr,0,len(arr))
print(arr)


//cpp
#include<iostream>
#include<vector>

using namespace::std;

void quickSort(vector<int> &arr ,int b, int e){
    if(e-b<=1){
        return;
    }
    int p = e-1;
    int f = b;
    for(int i = b;i<e-1;i++){
        if(arr[i]<arr[p]){
            int t = arr[i];
            arr[i] = arr[f];
            arr[f] = t;
            f+=1;
        }
    }
    int t = arr[p];
    arr[p] = arr[f];
    arr[f] = t;
    quickSort(arr,b,f);
    quickSort(arr,f+1,e);
    return;
}


int main(){

    vector<int> arr = {5,4,2,9,8,7,1};
    quickSort(arr,0,(int)arr.size());
    for(int i =0;i<arr.size();i++){
        cout<<arr[i]<<" ";
    }
    return 0;
}

//java
import java.util.List;
import java.util.ArrayList;
public class Main{
    public static void quickSort(ArrayList<Integer> arr, int b, int e){
        if(e-b <= 1){
            return;
        }
        int piv = e-1;
        int f = b;
        for(int i =b;i<e-1;i++){
            if(arr.get(piv) > arr.get(i)){
                Integer t = arr.get(f);
                arr.set(f,arr.get(i));
                arr.set(i,t);
                f++;
            }
        }
        Integer t = arr.get(f);
        arr.set(f,arr.get(piv));
        arr.set(piv,t);
        quickSort(arr,b,f);
        quickSort(arr,f,e);
    }
    public static void main(String[] args){
        ArrayList<Integer> arr = new ArrayList<>(List.of(5,4,6,7,3,2,8,9,10));
        System.out.println(arr);
        quickSort(arr,0,arr.size());
        System.out.println(arr);
    }
}
